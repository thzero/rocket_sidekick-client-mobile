{
  "appId": "com.rocketsidekick",
  "appName": "Rocket Sidekick",
  "webDir": "dist",
  "plugins": {
    "CapacitorSQLite": {
      "iosDatabaseLocation": "Library/rocketsidekick",
      "iosIsEncryption": false,
      "iosKeychainPrefix": "cap",
      "androidIsEncryption": false
