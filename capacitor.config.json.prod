{
  "appId": "com.rocketsidekick",
  "appName": "Rocket Sidekick",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "plugins": {
    "CapacitorSQLite": {
      "capConnectionOptions": {
        "database": "rocketsidekick"
      },
      "iosDatabaseLocation": "Library/rocketsidekick"
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com"]
    },
    "SplashScreen": {
      "launchAutoHide": false
    }
  }
}
